@page
@model IndexModel
@{
    ViewData["Title"] = "Asparagetti's Tour Page";
}

<div class="text-center">
    <h1 class="display-4">Asparagetti's Tour Page</h1>
    <h2 class="display-4">Thomas Bakken, Andrew Messner, Andrew Pfund</h2>
    <h2 class="display-4">Spring 2020</h2>

    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <a href="/Welcome">
        <img src="~/img/Welcome.jpg" alt="Welcome Page Image">
    </a>
    <a href="/Account">
        <img src="~/img/Account.jpg" alt="Account Page Image">
    </a>
    <a href="/Map">
        <img src="~/img/Map.jpg" alt="Map Page Image">
    </a>
    <a href="/Content1">
        <img src="~/img/Content.jpg" alt="Content Page Image">
    </a>

</div>



